{
  "name": "angular-formly-material",
  "version": "0.14.3",
  "description": "Material design templates for angular-formly",
  "main": "dist/formly-material.js",
  "scripts": {
    "build:dist": "webpack --progress",
    "build:prod": "webpack --progress --set-env-NODE_ENV=production",
    "build": "npm run build:dist && npm run build:prod",
    "lint:src": "eslint src/**/*.js",
    "lint:tests": "eslint tests/**/*.js --no-ignore",
    "lint": "npm run lint:src && npm run lint:tests",
    "coverage:codecov": "cat ./coverage/lcov.info | node_modules/.bin/codecov",
    "coverage:coveralls": "cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js",
    "coverage:codacy": "cat ./coverage/lcov.info | ./node_modules/.bin/codacy-coverage",
    "test": "karma start karma.conf.js --set-env-NODE_ENV=ci",
    "test:watch": "karma start karma.conf.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/formly-js/angular-formly-templates-material.git"
  },
  "keywords": [
    "AngularJs",
    "form",
    "formly",
    "json",
    "html",
    "material"
  ],
  "author": "Kamil Kisiela <mys.sterowiec@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/formly-js/angular-formly-templates-material/issues"
  },
  "homepage": "https://github.com/formly-js/angular-formly-templates-material#readme",
  "dependencies": {
    "angular": ">=1.6.5",
    "angular-messages": ">=1.6.5",
    "angular-animate": ">=1.6.5",
    "angular-aria": ">=1.6.5",
    "angular-material": "^1.1.4",
    "angular-formly": ">=8.4.1",
    "api-check": ">=7.5.5"
  },
  "devDependencies": {
    "angular-mocks": "^1.6.5",
    "argv-set-env": "^1.0.1",
    "babel": "^6.23.0",
    "babel-core": "^6.25.0",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^7.1.1",
    "babel-preset-es2015": "^6.24.1",
    "codacy-coverage": "^2.0.2",
    "codecov.io": "^0.1.6",
    "coveralls": "^2.13.1",
    "eslint": "^4.2.0",
    "eslint-config-airbnb": "^15.0.2",
    "html-loader": "^0.4.5",
    "isparta": "^4.0.0",
    "isparta-loader": "2.0.0",
    "jasmine-core": "^2.6.4",
    "jquery": "^3.2.1",
    "karma": "^1.7.0",
    "karma-babel-preprocessor": "^6.0.1",
    "karma-chrome-launcher": "^2.2.0",
    "karma-coverage": "^1.1.1",
    "karma-firefox-launcher": "^1.0.1",
    "karma-jasmine": "^1.1.0",
    "karma-jasmine-html-reporter": "^0.2.2",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^2.0.4",
    "lodash": "^4.17.4",
    "mys-common-tools": "latest",
    "webpack": "^3.3.0"
  }
}
